<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mytestapplication"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ccfl.mytestapplication.MainActivity"
            android:label="Staying On Track">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="Search Contact"
            android:name="com.ccfl.mytestapplication.ContactSearch">

        </activity>
        <activity android:label="Contact Detail"
            android:name="com.ccfl.mytestapplication.ContactInfo">

        </activity>
        <activity android:name="com.ccfl.mytestapplication.WebPage">

        </activity>
        <activity android:label="Directory"
            android:name="com.ccfl.mytestapplication.DirectoryPage">

       </activity>
    </application>

</manifest>